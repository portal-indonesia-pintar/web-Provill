{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@call-view/*": [
        "src/app/*"
      ],
      "@call-style/*": [
        "styles/*"
      ],
      "@call-pages/*": [
        "pages/*"
      ],
      "@call-assets/*": [
        "assets/*"
      ],
      "@call-common/*": [
        "common/*"
      ],
      "@call-context/*": [
        "context/*"
      ],
      "@call-components/*": [
        "components/*"
      ],
      "@call-helpers/*": [
        "helpers/*"
      ],
      "@call-hooks/*": [
        "hooks/*"
      ],
      "@call-layout/*": [
        "layout/*"
      ],
      "@call-routes/*": [
        "routes/*"
      ],
      "@call-styles/*": [
        "styles/*"
      ],
      "@call-type/*": [
        "type/*"
      ],
      "@call-auth/*": [
        "auth/*"
      ],
      "@call-root-lib/*": [
        "./*"
      ],
      "@call-admin/*": [
        "pages/admin/*"
      ],
      "@call-services/*": [
        "services/*"
      ],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
